<%-include('layouts/header')-%>
 <%-include('layouts/navbar')-%>

<%if(data.length==0){%>
  <div class="text-center mt-5"><h1>WELCOME TO L&T STUDENT PORTAL</h1></div>
  <% }; %>
<div class="container-fluid  card rounded shadow-sm">
  <%if(data.length!=0){%><table class="table">
    <thead>
      <tr>
        <th scope="col">Id</th>
        <th scope="col">Name</th>
        <th scope="col">Gender</th>
        <th scope="col">DOB</th>
        <th scope="col">City</th>
        <th scope="col">State</th>
        <th scope="col">Email</th>
        <th scope="col">Qualification</th>
        <th scope="col">Stream</th>
      </tr>
    </thead><% }; %>
   
    <tbody>
      <% data.forEach(function(user){ %>
      <tr>
        <th scope="row"><%= user.id %></th>
        <th scope="row"> <%= user.name %></h2></td>
        <th scope="row"><%= user.gender %></th>
        <th scope="row"><%= user.dob %></th>
        <th scope="row"><%= user.city %></th>
        <th scope="row"><%= user.state %></th>
        <th scope="row"><%= user.email %></th>
        <th scope="row"><%= user.qualification %></th>
        <th scope="row"><%= user.stream %></th>
      </tr>
      <% }); %>
    </tbody>
  </table>
</div>

<%-include('layouts/footer')-%>
